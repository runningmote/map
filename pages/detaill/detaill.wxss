/* pages/detaill/detaill.wxss */
page,.wrap,.mapWrap{
  width:100%;
  height:100%;
}
.mymap,.map{
  width:100%;
}
.mymap{
  height:85%;
}
.map{
  height:100%;
}
.pos{
  width:100%;
}
.posWrap{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin:20rpx;
}
.address{
  
  width:450rpx;
  font-size:40rpx;
  overflow:hidden;
  text-overflow:ellipsis; 
  white-space: nowrap;
}
.names{
  width:450rpx;
  display: inline-block;
  padding:15rpx 0;
  color:#666;
}
.runLine{
  width:100%;
}
.route{
  display:inline-block;
  width:200rpx;
  padding:10rpx;
  margin-left:10rpx;
  line-height: initial;
  margin-top:20rpx;
  text-align: center;
  font-size:50rpx;
  background:blue;
  border-radius: 80rpx;
  color:#fff;
}